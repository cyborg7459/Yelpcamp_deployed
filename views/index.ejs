    <body>
    
    <div class="container">
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    
    <div id="landing-header">
 		<h1 class="display-1">Welcome to YelpCamp!</h1>
		<a href="/home" class="btn btn-lg btn-success mt-3">Get Started</a>
    </div>
    
    <ul class="slideshow">
